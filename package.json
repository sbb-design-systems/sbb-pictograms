{
  "name": "@sbb-esta/pictograms",
  "version": "0.0.6",
  "description": "Pictogram package for the SBB organization",
  "repository": "https://github.com/sbb-design-systems/sbb-pictograms.git",
  "author": "ESTA",
  "license": "Apache-2.0",
